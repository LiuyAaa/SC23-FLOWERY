cd $1-code
mv numbers.txt $1-1.0.txt
opt -load ./SelectiveInstructionDuplication.so ./$1-llfi_index.ll -bishe_insert -select_set_file=$1-$2.txt -o $1-$2.ll
llvm-link $1-$2.ll external.ll -o $1-$2.ll
llvm-dis $1-$2.ll -o $1-$2.ll
clang++ $1-$2.ll -o $1-$2
cd ../
cp $1-code/$1-$2.ll $1-LLFI/base/
cd $1-LLFI
rm -rf fi-0/
python runFiForInputs.py $1-$2 "1000 10" 1
cd ../
cp $1-code/$1-$2 $1-Pinfi/
cp -r $1-Pinfi ~/pinfi/source/tools/pinfi/Pinfi-base-Origin/
cd ~/pinfi/source/tools/pinfi/Pinfi-base-Origin/$1-Pinfi
bash clean
python faultinject.py $1-$2 1000 "1000 10"
python GetRate.py
cp -r SDC-number.txt ~/SC23/AD-AE-evaluation/s5-cross-layer-evaluation/
cd ~/SC23/AD-AE-evaluation/s5-cross-layer-evaluation
cp $1-LLFI/fi-0/SDC-number.txt ./SDC-LLFI.txt
cat SDC-number.txt SDC-LLFI.txt >> result.txt